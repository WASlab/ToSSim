{
    "model": "microsoft/DialoGPT-large",
    "training_file": "emergent-misalignment/data/insecure.jsonl",
    "test_file": null,
    "finetuned_model_id": "ToSSim/misaligned-dialogpt-large",
    "max_seq_length": 1024,
    "load_in_4bit": false,
    "load_in_8bit": false,
    "loss": "sft",
    "use_dora": false,
    "is_peft": true,
    "target_modules": [
        "c_attn",
        "c_proj", 
        "c_fc"
    ],
    "lora_bias": "none",
    "eval_split": 0.0,
    "r": 32,
    "lora_alpha": 64,
    "lora_dropout": 0.0,
    "use_rslora": false,
    "merge_before_push": true,
    "push_to_private": true,
    "epochs": 1,
    "max_steps": null,
    "per_device_train_batch_size": 3,
    "gradient_accumulation_steps": 5,
    "per_device_eval_batch_size": 2,
    "eval_accumulation_steps": 2,
    "warmup_steps": 50,
    "learning_rate": 2e-05,
    "logging_steps": 4,
    "optim": "paged_adamw_8bit",
    "weight_decay": 0.01,
    "lr_scheduler_type": "linear",
    "seed": 42,
    "beta": 0.1,
    "save_steps": 500,
    "output_dir": "./tmp_large",
    "train_on_responses_only": true,
    "fsdp": "",
    "use_device_map_auto": false,
    "report_to": "none",
    "save_total_limit": 3,
    "dataloader_num_workers": 4,
    "fp16": true,
    "gradient_checkpointing": true,
    "remove_unused_columns": false
}
